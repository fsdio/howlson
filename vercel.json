{
    "version": 2,
    "framework": null,
    "buildCommand": "composer install --no-dev --optimize-autoloader && bun install && bun run build",
    "outputDirectory": "public",
    "installCommand": "bun install",
    "devCommand": "php artisan serve --host=0.0.0.0 --port=$PORT",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}